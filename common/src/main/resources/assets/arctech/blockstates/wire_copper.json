{
  "multipart": [
    {
      "apply": {
        "model": "arctech:block/wire_copper_center"
      }
    },
    {
      "apply": {
        "model": "arctech:block/wire_copper_edge",
        "uvlock": true
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "arctech:block/wire_copper_edge",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "arctech:block/wire_copper_edge",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "arctech:block/wire_copper_edge",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "arctech:block/wire_copper_edge",
        "uvlock": true,
        "x": 270
      },
      "when": {
        "up": "true"
      }
    },
    {
      "apply": {
        "model": "arctech:block/wire_copper_edge",
        "uvlock": true,
        "x": 90
      },
      "when": {
        "down": "true"
      }
    }
  ]
}
